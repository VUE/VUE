  <class name="java.awt.geom.Point2D$Float">
    <map-to xml="point"/>
    <field name="x" direct="true"><bind-xml node="attribute"/></field>
    <field name="y" direct="true"><bind-xml node="attribute"/></field>
  </class>
  <class name="java.awt.Point">
    <map-to xml="pointi"/>
    <field name="x" direct="true"><bind-xml node="attribute"/></field>
